      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{687B81A2-5B52-4E80-AA4C-71FFEBDDE4B4}"
          SQLTask:SqlStatementSource="update AFRS_Scheme&#xA;set AFRS_Scheme.afrs_meg_id = (select afrs_meg_id_A &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;from bulkAssignments&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;where bulkAssignments.AFRS_MonthlyAssignmentxWalk_Id =  AFRS_Scheme.afrs_xwalk_id),&#xA;&#xA;AFRS_Scheme.afrs_svc_id = (select afrs_svc_id_A &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;           from bulkAssignments&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;           where bulkAssignments.AFRS_MonthlyAssignmentxWalk_Id =  AFRS_Scheme.afrs_xwalk_id),&#xA;&#xA;AFRS_Scheme.afrs_sof_id = (select afrs_sof_id_A &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;          from bulkAssignments&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;          where bulkAssignments.AFRS_MonthlyAssignmentxWalk_Id =  AFRS_Scheme.afrs_xwalk_id),&#xA;&#xA;AFRS_Scheme.createby = (select createby &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;      from bulkAssignments&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;      where bulkAssignments.AFRS_MonthlyAssignmentxWalk_Id = AFRS_Scheme.afrs_xwalk_id),&#xA;&#xA;AFRS_Scheme.ModifiedDate = (select createDate &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;          from bulkAssignments&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;          where bulkAssignments.AFRS_MonthlyAssignmentxWalk_Id = AFRS_Scheme.afrs_xwalk_id)&#xA;&#xA;where scheme = 'A'&#xA;and AFRS_Scheme.EndFcCycle = '9999'&#xA;and AFRS_Scheme.afrs_xwalk_id in (select AFRS_MonthlyAssignmentxWalk_Id&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;      from bulkAssignments)&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
